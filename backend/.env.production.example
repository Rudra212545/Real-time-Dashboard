# Backend Production Environment Variables

# Server
PORT=3000
NODE_ENV=production

# CORS - Add your frontend domain
FRONTEND_ORIGIN=https://your-frontend-domain.vercel.app

# JWT
JWT_SECRET=your_production_jwt_secret_here
JWT_ISSUER=microbridge.production
JWT_EXP=1h

# HMAC
HMAC_SECRET=your_production_hmac_secret_here
HMAC_WINDOW_MS=15000

# Agent Secrets
HINT_AGENT_SECRET=your_hint_agent_secret
NAV_AGENT_SECRET=your_nav_agent_secret
PREDICT_AGENT_SECRET=your_predict_agent_secret
RULE_AGENT_SECRET=your_rule_agent_secret
ENGINE_SHARED_SECRET=your_engine_shared_secret

# Database
MONGO_URI=your_mongodb_connection_string
